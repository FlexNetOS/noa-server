    "rotation"
  ],
  "author": "Noa Server Team",
  "license": "MIT",
  "dependencies": {
    "node-vault": "^0.10.2",
    "@aws-sdk/client-secrets-manager": "^3.658.1",
    "@azure/keyvault-secrets": "^4.8.0",
    "@azure/identity": "^4.4.1",
    "@google-cloud/secret-manager": "^5.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
