{
  "name": "@noa/ui",
  "version": "1.0.0",
  "type": "module",
  "description": "Advanced UI components for NOA Server - Chat, Files, Analytics, and Visualizations",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.css"
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "tsc && vite build --mode lib",
    "build:analyze": "tsc && vite build --mode analyze",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:gate": "VITEST_DISABLE_THRESHOLDS=1 vitest run --coverage tests/gated",
    "cleanup:unused": "tsx scripts/cleanup-unused.ts",
    "cleanup:unused:live": "tsx scripts/cleanup-unused.ts --live",
    "cleanup:unused:quiet": "tsx scripts/cleanup-unused.ts --quiet"
  },
  "dependencies": {
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.7.9",
    "bits-ui": "^1.1.6",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "framer-motion": "^11.15.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.469.0",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^4.9.124",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.9.4",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-zoom-pan-pinch": "^3.7.0",
    "recharts": "^2.15.1",
    "rehype-highlight": "^7.0.2",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tsx": "^4.20.6",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/d3": "^7.4.3",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-remove-properties": "^0.3.1",
    "eslint": "^9.18.0",
    "fake-indexeddb": "^6.0.0",
    "jest-axe": "^10.0.0",
    "msw": "^2.8.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^4.0.11",
    "typescript": "^5.7.3",
    "vite": "^7.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-compression2": "^2.3.0",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
