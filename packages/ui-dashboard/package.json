{
  "name": "ui-dashboard",
  "version": "0.2.0",
  "private": true,
  "description": "Real-time Claude Suite automation dashboard with MCP integration - WCAG 2.1 AA Compliant",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "build:python": "python3 ../../scripts/ui/build_dashboard.py",
    "start": "vite --host 0.0.0.0 --port 3000",
    "test": "jest --config jest.config.cjs",
    "test:a11y": "jest --config jest.config.cjs --testPathPattern=accessibility",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:gate": "jest --config jest.gate.config.cjs",
    "lighthouse": "lhci autorun",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:a11y": "eslint src --ext .ts,.tsx --plugin jsx-a11y"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "framer-motion": "^11.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.4.4",
    "recharts": "^2.15.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.1",
    "@jest/globals": "^29.7.0",
    "@lhci/cli": "^0.15.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.8.3",
    "eslint": "^8.55.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^6.0.11"
  }
}
