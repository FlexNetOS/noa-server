{
  "name": "@noa/cdn-manager",
  "version": "1.0.0",
  "description": "Multi-CDN management and asset optimization system for Noa Server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy": "node --loader ts-node/esm scripts/deploy-assets.ts"
  },
  "keywords": [
    "cdn",
    "cloudfront",
    "cloudflare",
    "fastly",
    "akamai",
    "asset-optimization",
    "image-optimization",
    "cache-control"
  ],
  "author": "Noa Server Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.515.0",
    "@aws-sdk/client-s3": "^3.515.0",
    "cloudflare": "^2.9.1",
    "axios": "^1.6.7",
    "sharp": "^0.33.2",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "imagemin-avif": "^0.1.6",
    "terser": "^5.27.0",
    "csso": "^5.0.5",
    "html-minifier-terser": "^7.2.0",
    "mime-types": "^2.1.35",
    "glob": "^10.5.0",
    "winston": "^3.11.0",
    "chalk": "^5.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/mime-types": "^2.1.4",
    "@vitest/coverage-v8": "^1.2.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2"
  }
}
