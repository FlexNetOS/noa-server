{
  "name": "@noa/ai-inference-api",
  "version": "1.0.0",
  "description": "REST API for AI model inference and management with OpenAPI documentation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
