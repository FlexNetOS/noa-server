{
  "name": "mcp-server-skeleton",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node --import ./dist/otel-preload.js dist/server.js",
    "dev": "tsx src/server.ts",
    "dev:otel": "tsx -r ./src/otel-preload.ts src/server.ts",
    "export:rtt": "node dist/export_rtt.js"
  },
  "dependencies": {
    "zod": "^3.23.8",
    "ajv": "^8.17.1",
    "@modelcontextprotocol/sdk": "*",
    "@opentelemetry/api": "^1.9.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "tsx": "^4.19.2",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.54.0",
    "@types/node": "^22.7.8"
  }
}
